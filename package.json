{
	"name": "git-helper",
	"displayName": "Git Helper",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:git-helper-sidebar",
		"onCommand:git-helper.helloWorld",
		"onCommand:git-helper.refresh"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "git-helper-sidebar-view",
					"title": "Git Helper",
					"icon": "media/icon.svg"
				}
			]
		},
		"views": {
			"git-helper-sidebar-view": [
				{
					"type": "webview",
					"id": "git-helper-sidebar",
					"name": "Git Helper",
					"icon": "media/icon.svg",
					"contextualTitle": "Git Helper"
				}
			]
		},
		"commands": [
			{
				"command": "git-helper.helloWorld",
				"title": "Hello World",
				"category": "Git Helper"
			},
			{
				"command": "git-helper.refresh",
				"title": "Refresh",
				"category": "Git Helper"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@rollup/plugin-typescript": "^8.2.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "4.16.1",
		"@typescript-eslint/parser": "4.16.1",
		"babel-eslint": "10.1.0",
		"child_process": "^1.0.2",
		"concurrently": "^6.0.0",
		"eslint": "^7.19.0",
		"eslint-config-airbnb": "18.2.1",
		"eslint-config-airbnb-typescript": "12.3.1",
		"eslint-config-prettier": "8.1.0",
		"eslint-import-resolver-typescript": "2.4.0",
		"eslint-plugin-flowtype": "5.3.1",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jest": "24.1.5",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-prettier": "3.3.1",
		"eslint-webpack-plugin": "2.5.2",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"node-sass": "^5.0.0",
		"prettier": "2.2.1",
		"prettier-eslint": "12.0.0",
		"prettier-eslint-cli": "5.0.0",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-svelte-svg": "^0.2.3",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.32.8",
		"svelte": "^3.34.0",
		"svelte-check": "^1.1.36",
		"svelte-preprocess": "^4.6.9",
		"ts-loader": "^8.0.14",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"fs": "0.0.1-security"
	}
}
